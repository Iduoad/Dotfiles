    qutebrowser
super + shift + e
	$BROWSER
super + shift + g
	gimp
super + shift + v
	virtualbox
super + shift + z
	zeal

super + shift + d
	rofi-library

super + e
    rofi-emoji

super + shift + m
	audioctl mute
super + shift + w
	rofi-radio

super + x
	mpc pause; pauseallmpv; lock
super + shift + x
	prompt "Shutdown computer?" "sudo -A shutdown -h now"
super + shift + BackSpace
	prompt "Reboot computer?" "sudo -A shutdown -r now"
Print
    scrot -e 'mv $f ~/Pictures'

XF86Launch1
	xset dpms force off
