super + Return
	$TERMINAL
super + shift + Return
	$TERMINAL -e tmux
super + d
	rofi -show run
super + shift + f
	firefox -private-window
super + i
	$TERMINAL -e htop
super + m
	$TERMINAL -e ncmpcpp
super + n
	$TERMINAL -e newsboat
super + r
	$TERMINAL -e $FILE
super + t
	$TERMINAL -e transmission-remote-cli
super + y
	$TERMINAL -e calcurse -D ~/.config/calcurse

